{
  "compilerOptions": {
    "baseUrl": "/src",
    "outDir": "./dist",
    "rootDir": "./src",
    "sourceMap": true,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "lib": ["es2016"],
  "exclude": ["node_modules"],
  "include": ["src/**/*"],
}
